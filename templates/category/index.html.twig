<script>
 $(document).ready(function() {
  // Cacher tous les menus d√©roulants au chargement de la page
  $('.dropdown-menu').hide();

  // Ajouter un l√©ger d√©lai (en millisecondes) entre le survol et l'affichage du menu d√©roulant
  var hoverDelay = 200; // Par exemple, un d√©lai de 200 ms

  var timeout;

  // G√©rer le survol des √©l√©ments de menu avec un d√©lai
  $('.nav-item.dropdown').hover(
    function() {
      var $menuItem = $(this);

      clearTimeout(timeout); // Effacer le d√©lai pr√©c√©dent s'il existe

      timeout = setTimeout(function() {
        // Fermer tous les menus d√©roulants ouverts
        $('.nav-item.dropdown').removeClass('show');
        $('.dropdown-menu').hide();

        // Ouvrir le menu d√©roulant de l'√©l√©ment survol√©
        $menuItem.addClass('show');
        $menuItem.find('.dropdown-menu').show();
      }, hoverDelay);
    },
    function() {
      clearTimeout(timeout); // Effacer le d√©lai pr√©c√©dent s'il existe

      var $menuItem = $(this);

      timeout = setTimeout(function() {
        // Fermer le menu d√©roulant lorsque le curseur quitte l'√©l√©ment
        $menuItem.removeClass('show');
        $menuItem.find('.dropdown-menu').hide();
      }, hoverDelay);
    }
  );

  // Fermer tous les menus d√©roulants lorsque l'utilisateur clique ailleurs dans la page
  $(document).click(function(e) {
    var $target = $(e.target);
    if (!$target.is('.navbar-brand') && !$target.parents('.navbar-brand').length) {
      $('.dropdown-menu').hide();
    }
  });
});
</script>
{% endblock %}